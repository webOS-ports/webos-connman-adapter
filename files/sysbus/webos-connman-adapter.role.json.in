{
    "role": {
        "exeName":"@WEBOS_INSTALL_SBINDIR@/webos-connman-adapter",
        "type": "regular",
        "allowedNames": ["com.palm.wifi", "com.palm.connectionmanager"]
    },
    "permissions": [
        {
            "service":"com.palm.wifi",
            "outbound":["com.webos.settingsservice",
                        "com.palm.wifi",
                        "com.palm.connectionmanager"]
        },
        {
            "service":"com.palm.connectionmanager",
            "outbound":["com.webos.settingsservice",
                        "com.palm.wifi"]
        }
    ]
}
